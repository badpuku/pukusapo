{
  "name": "puku-form",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "npm run gen:icons",
    "build": "react-router build",
    "deploy": "npm run build && wrangler deploy",
    "dev": "react-router dev",
    "gen:icons": "svgr -d app/components/icons public/icons",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "format": "npm-run-all2 -s format:*",
    "format:lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "format:code": "prettier --write .",
    "typecheck": "react-router typegen && tsc",
    "typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:generate:custom": "drizzle-kit generate --custom",
    "db:migrate": "supabase migration up",
    "db:seed": "dotenv -e .dev.vars -- tsx ./app/db/seeds",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-router/cloudflare": "^7.2.0",
    "@sentry/react": "^9.24.0",
    "@sentry/react-router": "^9.24.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.6",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "drizzle-seed": "^0.3.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.460.0",
    "neverthrow": "^8.1.1",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "remix-auth": "^4.1.0",
    "supabase": "^1.207.9",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@cloudflare/workers-types": "^4.20250224.0",
    "@react-router/dev": "^7.2.0",
    "@react-router/fs-routes": "^7.2.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@svgr/cli": "^8.1.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@tailwindcss/vite": "^4.0.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.1",
    "npm-run-all2": "^7.0.1",
    "pg": "^8.13.3",
    "playwright": "^1.51.1",
    "postcss": "^8.4.38",
    "prettier": "3.3.3",
    "storybook": "^8.4.5",
    "storybook-addon-remix-react-router": "^4.0.1",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "wrangler": "^3.106.0"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "volta": {
    "node": "20.18.0"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
